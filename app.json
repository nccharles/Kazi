{
  "expo": {
    "name": "Kazi",
    "description": "Kazi application.",
    "slug": "Kazi",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#227093",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "scheme": "limitlessafricanapps",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.limitlessafricanapps.Kazi",
      "config": {
        "googleMapsApiKey": "AIzaSyCy6H2ah8PtVYEl6gsbYNGaTCTiZsgPttw"
      }
    },
    "notification": {
      "icon": "./assets/images/notify.png",
      "color": "#227093",
      "androidMode": "collapse",
      "androidCollapsedTitle": "#{unread_notifications} new messages"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#227093"
    },
    "android": {
      "package": "com.limitlessafricanapps.Kazi",
      "versionCode": 3,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_INTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyArRwHRrKfyvF9zq4RRtI5NB_YNJ4Cl3Ug"
        }
      }
    }
  }
}